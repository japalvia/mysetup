partial default alphanumeric_keys
xkb_symbols "basic" {
    include "fi"
    include "ctrl(nocaps)"

    name[Group1]="FI layout for Linux dev";

    replace key <AD01> { [  q,  Q,  asciitilde ] };
    replace key <AD02> { [  w,  W,  slash,   backslash  ] };
    replace key <AD03> { [  e,  E,  braceleft  ] };
    replace key <AD04> { [  r,  R,  braceright  ] };
    replace key <AE07> { [  7,	slash ] };
    replace key <AE08> { [  8, NoSymbol, NoSymbol, Nosymbol ] };
    replace key <AE09> { [  9, NoSymbol, NoSymbol, Nosymbol ] };
    replace key <AE10> { [  0,  equal ] };
    replace key <AE12> { [  acute,  grave] };
    replace key <AC01> { [  a,	A,	parenleft  ] };
    replace key <AC02> { [  s,	S,	parenright ] };
    replace key <AC03> { [  d,	D,	bracketleft ] };
    replace key <AC04> { [  f,	F,	bracketright] };
    replace key <AD12> { [ asciicircum, Insert, NoSymbol, NoSymbol ] };
    replace key <FK08>   { [ XF86AudioMute, F8  ] };
    replace key <FK09>   { [ XF86AudioLowerVolume,  F9  ] };
    replace key <FK10>   { [ XF86AudioRaiseVolume,  F10  ] };
    replace key <FK11>   { [ XF86AudioPlay,  F11  ] };
    replace key <FK12>   { [ F12, XF86Display  ] };
    replace key <PAUS> { [ Insert, NoSymbol, NoSymbol, NoSymbol ] };
};
